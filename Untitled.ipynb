    "import csv\n",
    "from collections import deque\n",
    "\n",
    "q = deque()\n",
    "reader = csv.reader(iter(q.popleft, None))\n",
    "\n",
    "q.append(\"a;b;c\")\n",
    "print(reader.__next__())\n",
    "\n",
    "q.append(\"c;d;e\")\n",
    "print(reader.__next__())"
   ]
